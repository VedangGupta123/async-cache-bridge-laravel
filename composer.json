{
    "name": "fyennyi/async-cache-bridge-laravel",
    "description": "Laravel bridge for Fyennyi AsyncCache.",
    "type": "library",
    "license": "LicenseRef-CSSM-Unlimited-2.0",
    "authors": [
        {
            "name": "Serhii Cherneha",
            "email": "chernegasergiy3@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "fyennyi/async-cache-php": "dev-main",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0",
        "illuminate/contracts": "^9.0 || ^10.0 || ^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "orchestra/testbench": "^9.0",
        "friendsofphp/php-cs-fixer": "^3.93"
    },
    "autoload": {
        "psr-4": {
            "Fyennyi\\AsyncCache\\Bridge\\Laravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fyennyi\\AsyncCache\\Bridge\\Laravel\\AsyncCacheServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
